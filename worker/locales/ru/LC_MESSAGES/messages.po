msgstr "<b>Данные успешно удалены.</b>"

#: handlers/settings/data.py:33
msgid "Members"
msgstr "участников"

#: handlers/settings/data.py:37
msgid "Messages"
msgstr "сообщений"

#: handlers/settings/data.py:46
msgid "enabled"
msgstr "включена"

#: handlers/settings/data.py:49
msgid "disabled"
msgstr "отключена"

